{
    "manifest_version": 2,
    "name": "youtube filteringer",
    "version": "1.0",
    "description": "filter youtube",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "./deleteshorts.js"
            ]
        }
    ],
    "background": [
        {
            "css":["style.css"]
        }
    ]
}