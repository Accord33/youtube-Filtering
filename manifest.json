{
    "manifest_version": 3,
    "name": "youtube filteringer",
    "version": "1.0",
    "description": "filter youtube",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "popup.js"
            ]
        }
    ],
    "background": [
        {
            "css": ["popup.css"]
        }
    ]
}